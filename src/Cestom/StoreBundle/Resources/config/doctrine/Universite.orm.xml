<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Cestom\StoreBundle\Entity\Universite" table="UNIVERSITE">
    <id name="iduniv" type="integer" column="IDUNIV">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomuniv" type="string" column="NOMUNIV" length="255" nullable="false"/>
    <field name="contactuniv" type="string" column="CONTACTUNIV" length="255" nullable="true"/>
    <field name="addresseuniv" type="string" column="ADDRESSEUNIV" length="500" nullable="true"/>
  </entity>
</doctrine-mapping>
