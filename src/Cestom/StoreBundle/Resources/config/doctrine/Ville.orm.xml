<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Cestom\StoreBundle\Entity\Ville" table="VILLE">
    <unique-constraints>
      <unique-constraint name="NOMVILLE" columns="NOMVILLE"/>
    </unique-constraints>
    <id name="idville" type="integer" column="IDVILLE">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomville" type="string" column="NOMVILLE" length="255" nullable="false"/>
  </entity>
</doctrine-mapping>
