<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Cestom\StoreBundle\Entity\Telephone" table="TELEPHONE">
    <indexes>
      <index name="FK_TELEPHONE_MEMBRE" columns="IDMEMBRE"/>
    </indexes>
    <id name="numeroTel" type="string" column="NUMERO_TEL" length="20">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="idmembre" target-entity="Membre">
      <join-columns>
        <join-column name="IDMEMBRE" referenced-column-name="IDMEMBRE"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
